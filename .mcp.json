{
    "mcpServers": {
        "next-devtools": {
            "command": "npx",
            "args": [
                "-y",
                "next-devtools-mcp@latest"
            ]
        },
        "next-app": {
            "url": "http://localhost:3000/_next/mcp",
            "transport": {
                "type": "sse"
            },
            "headers": {
                "Accept": "text/event-stream, application/json"
            }
        }
    }
}